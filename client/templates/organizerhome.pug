extends layout.pug

block css
    link(rel='stylesheet' href='/styles/admin.css')
block main-navigation  
  div.navbar-start
    a.navbar-item(href="/organizer")= "Home"
  div.navbar-end
    div.navbar-item
      div.buttons
        a.button.is-info(href="/")= "Logout"
block side-navigation    
  p.menu-label= "General"                   
  ul.menu-list
    li
      a.is-active(href='#')= "Dashboard"
    li
      a(href='/organizer/userlist')= "User Management"
  p.menu-label= "Event Management"
  ul.menu-list
    li
      a(href='/organizer/eventsettings')= "Events"
    li
      a(href='/organizer/analyze')= "Analyze"
block breadcrump
  ul
    li
      a(href="/organizer")= "Home"    
    li.is-active
      a(href="#" aria-current="page")= "Dashboard"      
block main-content
  section(class="hero is-info welcome is-small")
    div.hero-body
      div.container
        h1.title Hello, Admin.
        h2.subtitle this is your dashboard!
          
